{
  "extends": "airbnb",
  "env": {
    "browser": true,
    "node": true
  },
  "parser": "babel-eslint",
  "rules": {
    "arrow-parens": 0,
    "implicit-arrow-linebreak": 0,
    "comma-dangle": 0,
    "no-nested-ternary": 0,
    "react/forbid-prop-types": 0,
    "react/require-default-props": 0
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
